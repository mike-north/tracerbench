{
  "name": "tracerbench-cli",
  "version": "2.0.0-beta.18",
  "description": "CLI for Tracerbench",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/TracerBench/tracerbench",
  "bugs": "https://github.com/TracerBench/tracerbench/issues",
  "license": "BSD-2-Clause",
  "author": "Kris Selden @krisselden",
  "files": [
    "bin",
    "dist",
    "src",
    "oclif.manifest.json",
    "tb-schema.json"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "tracerbench": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1.5.8",
    "@oclif/config": "^1.12.0",
    "@oclif/plugin-help": "^2.1.4",
    "chalk": "^2.4.2",
    "cli-table3": "^0.5.1",
    "d3-array": "^2.0.3",
    "d3-color": "^1.2.3",
    "d3-scale": "^3.0.0",
    "fs-extra": "^7.0.1",
    "inquirer": "^6.2.2",
    "json-query": "^2.2.2",
    "json5": "^2.1.0",
    "jstat": "^1.8.3",
    "listr": "^0.14.3",
    "log-symbols": "^3.0.0",
    "ora": "^3.4.0",
    "path": "0.12.7",
    "tracerbench": "^2.0.0-beta.18",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.0",
    "@oclif/test": "^1.2.2",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/chai-fs": "^2.0.1",
    "@types/chalk": "^2.2.0",
    "@types/d3-array": "^1.2.6",
    "@types/d3-color": "^1.2.2",
    "@types/d3-scale": "^2.1.1",
    "@types/fs-extra": "^5.0.4",
    "@types/inquirer": "^0.0.44",
    "@types/json-query": "^2.2.0",
    "@types/json5": "*",
    "@types/listr": "^0.13.0",
    "@types/log-symbols": "^3.0.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/ora": "^3.2.0",
    "chai": "^4.2.0",
    "chai-fs": "^2.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "prettier": "^1.16.4",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.4000",
    "typescript-json-schema": "^0.37.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "commands": "./dist/src/commands",
    "bin": "tracerbench",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "yarn build && yarn build:schema && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha \"test/commands/**/*.test.ts\" \"test/helpers/**/*.test.ts\"",
    "test:helpers": "nyc --extension .ts mocha \"test/helpers/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "build": "rm -rf ./dist && tsc -b",
    "build:schema": "typescript-json-schema \"src/helpers/tb-config.ts\" ITBConfig --out ./tb-schema.json",
    "plot": "./bin/plot.R \"tracerbench-results/compare.json\" \"test/tbconfig.json\""
  },
  "gitHead": "e9e301b7d4a75ac252fe69cf533668e42432ea8d"
}
